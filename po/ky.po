#: ../src/iso2022.c:779 ../src/iso2022.c:787 ../src/iso2022.c:818
#: ../src/vte.c:2277
#, c-format
msgid "Unable to convert characters from %s to %s."
msgstr "Символдорду \"%s\" көптүгүнөн \"%s\" көптүгүнө которууга болбоду."

#: ../src/iso2022.c:1472
#, c-format
msgid "Attempt to set invalid NRC map '%c'."
msgstr "Туура эмес '%c'-NRC картасын орнотуу аракети"

#. Application signalled an "identified coding system" we haven't heard of.  See ECMA-35 for gory details.
#: ../src/iso2022.c:1502
msgid "Unrecognized identified coding system."
msgstr "Коддо системасы таанылган жок"

#: ../src/iso2022.c:1561 ../src/iso2022.c:1588
#, c-format
msgid "Attempt to set invalid wide NRC map '%c'."
msgstr "'%c' - NRC картасынын туура эмес жазылыгын орнотуу аракети"

#: ../src/trie.c:409
#, c-format
msgid "Duplicate (%s/%s)!"
msgstr "Кайталоо (%s/%s)!"

#. Bail back to normal mode.
#: ../src/vteapp.c:981
msgid "Could not open console.\n"
msgstr "Консолду ачууга болбоду.\n"

#: ../src/vteapp.c:1077
msgid "Could not parse the geometry spec passed to --geometry"
msgstr ""
"--geometry параметрине берилген геометриялык параметрлерди ажыратып окууга "
"болбоду"

#: ../src/vte.c:1303
#, c-format
msgid "Error compiling regular expression \"%s\"."
msgstr "\"%s\" регулярдык туюнтмасын компиляциялоо катасы. "

#. Translators: %s is replaced with error message returned by strerror().
#: ../src/vte.c:4591
#, c-format
msgid "Error reading from child: %s."
msgstr "Туунду процесстен окуу катасы: %s."

#: ../src/vte.c:4718
msgid "Unable to send data to child, invalid charset convertor"
msgstr ""
"Туунду процесске маалымат берүүгө болбоду; коддук жадыбалдын туура эмес "
"которгучу"

#: ../src/vte.c:4729 ../src/vte.c:5791
#, c-format
msgid "Error (%s) converting data for child, dropping."
msgstr "Туунду процесс үчүн маалыматты конвертация катасы (%s), ыргытылууда."

#: ../src/vte.c:7976
#, c-format
#| msgid "Error reading PTY size, using defaults: %s."
msgid "Error reading PTY size, using defaults: %s\n"
msgstr "PTY өлчөмүн окуу катасы, абалкы маанилер колдонулууда: %s\n"

#. Aaargh.  We're screwed.
#: ../src/vte.c:13794
msgid "_vte_conv_open() failed setting word characters"
msgstr "Сөздө символду орнотууда _vte_conv_open() функциясынын катасы"

#~ msgid "Error adding `%s' to environment, continuing."
#~ msgstr "\"%s\" өзгөрмөсүн чөйрөгө кошуу катасы, улантуу."

#~ msgid "can not run %s"
#~ msgstr "\"%s\" иштетүүгө болбоду"

#~ msgid "Error creating signal pipe."
#~ msgstr "Сигналдар каналын түзүү катасы."

#~ msgid "No handler for control sequence `%s' defined."
#~ msgstr "\"%s\" башкаруу удаалаштыгы үчүн аткаруучу аныкталган эмес."

#~ msgid "Error setting PTY size: %s."
#~ msgstr "PTY өлчөмүн орнотуу катасы: %s."

#~ msgid "Unknown pixel mode %d.\n"
#~ msgstr "%d - белгисиз пиксел режими.\n"

#~ msgid "Can not draw character U+%04x.\n"
#~ msgstr "U+%04x коддуу символду чагылдырууга болбоду.\n"

#~ msgid "Got unexpected (key?) sequence `%s'."
#~ msgstr "Күтүүсүз (ачкыч?) удаалаштык алынды: \"%s\"."
